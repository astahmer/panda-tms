#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/astahmer/dev/reproductions/panda-tms/node_modules/.pnpm/@pandacss+dev@0.9.0_@internationalized+date@3.3.0_@types+node@20.3.3_@types+react-dom@18.2.6__xxc6dhhfpflz6whypgijviqsti/node_modules/@pandacss/dev/node_modules:/Users/astahmer/dev/reproductions/panda-tms/node_modules/.pnpm/@pandacss+dev@0.9.0_@internationalized+date@3.3.0_@types+node@20.3.3_@types+react-dom@18.2.6__xxc6dhhfpflz6whypgijviqsti/node_modules/@pandacss/node_modules:/Users/astahmer/dev/reproductions/panda-tms/node_modules/.pnpm/@pandacss+dev@0.9.0_@internationalized+date@3.3.0_@types+node@20.3.3_@types+react-dom@18.2.6__xxc6dhhfpflz6whypgijviqsti/node_modules:/Users/astahmer/dev/reproductions/panda-tms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/astahmer/dev/reproductions/panda-tms/node_modules/.pnpm/@pandacss+dev@0.9.0_@internationalized+date@3.3.0_@types+node@20.3.3_@types+react-dom@18.2.6__xxc6dhhfpflz6whypgijviqsti/node_modules/@pandacss/dev/node_modules:/Users/astahmer/dev/reproductions/panda-tms/node_modules/.pnpm/@pandacss+dev@0.9.0_@internationalized+date@3.3.0_@types+node@20.3.3_@types+react-dom@18.2.6__xxc6dhhfpflz6whypgijviqsti/node_modules/@pandacss/node_modules:/Users/astahmer/dev/reproductions/panda-tms/node_modules/.pnpm/@pandacss+dev@0.9.0_@internationalized+date@3.3.0_@types+node@20.3.3_@types+react-dom@18.2.6__xxc6dhhfpflz6whypgijviqsti/node_modules:/Users/astahmer/dev/reproductions/panda-tms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@pandacss/dev/bin.js" "$@"
else
  exec node  "$basedir/../@pandacss/dev/bin.js" "$@"
fi
